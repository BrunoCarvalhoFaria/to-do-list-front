{"ast":null,"code":"import TaskUnit from \"./Task/taskUnit.vue\";\nimport AddTask from './AddTask/addTask.vue';\nimport { mapState, mapMutations } from 'vuex';\nimport ModalTaskInfo from '../common/modal/modalTaskInfo.vue';\nexport default {\n  beforeMount() {\n    this.loadTaskList();\n  },\n  components: {\n    TaskUnit,\n    AddTask,\n    ModalTaskInfo\n  },\n  computed: {\n    ...mapState({\n      taskList: state => state.taskList\n    })\n  },\n  data: () => ({\n    editTaskModalVisible: false,\n    editIndex: 0\n  }),\n  methods: {\n    ...mapMutations(['changeTaskStatus', 'deleteTask', 'editTask', 'loadTaskList']),\n    editTaskCall: function (index) {\n      this.editTaskModalVisible = false;\n      this.editTask(this.$store.state.taskList[index]._id);\n      this.editIndex = 0;\n    },\n    defineEditTask: function (index) {\n      this.$store.commit('newTaskMount', this.$store.state.taskList[index]);\n      this.$refs.taskTemp = this.$store.state.taskList[index];\n      this.editIndex = index;\n      this.editTaskModalVisible = true;\n    },\n    changeEditTaskModalVisible: function () {\n      this.editTaskModalVisible = false;\n    }\n  }\n};","map":{"version":3,"mappings":"AAqCA;AACA;AACA;AACA;AAGA;EAEAA;IACA;EAEA;EAEAC;IACAC;IACAC;IACAC;EACA;EAEAC;IACA;MACAC;IACA;EACA;EAGAC;IACAC;IACAC;EACA;EAEAC;IACA,iBACA,oBACA,cACA,YACA,eACA;IACAC;MACA;MACA;MACA;IACA;IACAC;MACA;MACA;MACA;MACA;IACA;IACAC;MACA;IAAA;EACA;AACA","names":["beforeMount","components","TaskUnit","AddTask","ModalTaskInfo","computed","taskList","data","editTaskModalVisible","editIndex","methods","editTaskCall","defineEditTask","changeEditTaskModalVisible"],"sourceRoot":"src/components/FilteredTasks","sources":["taskList.vue"],"sourcesContent":["<template>\n  <div >\n    <hr>    \n    <AddTask/>    \n    \n    <v-row>\n      <v-col       \n        cols=\"12\"\n        sm=\"6\"\n        md=\"3\"              \n        v-for=\"(task, index) in taskList\" \n        :key=\"index\">\n        <TaskUnit \n              :name = \"taskList[index].name\"\n              :date = \"taskList[index].date\"\n              :description = \"taskList[index].description\"\n              :done = \"taskList[index].done\"\n              @changeStatus = \"changeTaskStatus(taskList[index]._id, index)\"\n              @deleteTask = \"deleteTask(taskList[index]._id)\"\n              @editTask = \"defineEditTask(index)\"\n              />         \n      </v-col>\n      <v-dialog\n          v-model=\"editTaskModalVisible\"      \n        >\n          <v-card class=\"pa-10\" outlined shaped>\n            <ModalTaskInfo              \n              label='Editar tarefa:'\n              @save=\"editTaskCall(editIndex)\"\n              @modalVisible=\"changeEditTaskModalVisible()\"/>\n          </v-card>\n        </v-dialog> \n    </v-row>     \n  </div>\n</template>\n\n<script>\nimport TaskUnit from \"./Task/taskUnit.vue\";\nimport AddTask from './AddTask/addTask.vue';\nimport {mapState, mapMutations} from 'vuex'\nimport ModalTaskInfo from '../common/modal/modalTaskInfo.vue'\n\n\nexport default { \n\n  beforeMount(){    \n    this.loadTaskList()\n    \n  },\n\n  components: {\n    TaskUnit,\n    AddTask,\n    ModalTaskInfo\n  },\n\n  computed: {\n    ...mapState({\n      taskList: state => state.taskList\n    })    \n  },\n\n\n  data: () => ({\n    editTaskModalVisible: false,\n    editIndex: 0\n  }),\n\n  methods:{\n    ...mapMutations([\n      'changeTaskStatus',\n      'deleteTask',\n      'editTask',\n      'loadTaskList'\n    ]),\n    editTaskCall: function(index) {      \n      this.editTaskModalVisible = false;\n      this.editTask(this.$store.state.taskList[index]._id)\n      this.editIndex = 0;\n    },\n    defineEditTask: function(index){\n      this.$store.commit('newTaskMount',this.$store.state.taskList[index]); \n      this.$refs.taskTemp = this.$store.state.taskList[index];        \n      this.editIndex = index;\n      this.editTaskModalVisible = true;\n    },\n    changeEditTaskModalVisible: function () {      \n      this.editTaskModalVisible = false },         \n  }\n};\n\n\n</script>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}