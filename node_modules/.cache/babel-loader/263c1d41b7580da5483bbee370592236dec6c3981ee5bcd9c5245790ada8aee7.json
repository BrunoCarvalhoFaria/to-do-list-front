{"ast":null,"code":"import TaskUnit from \"./Task/taskUnit.vue\";\nimport AddTask from './AddTask/addTask.vue';\nimport { mapState, mapMutations } from 'vuex';\nimport ModalTaskInfo from '../common/modal/modalTaskInfo.vue';\nimport api from \"./services/api\";\nexport default {\n  async beforeMount() {\n    console.log(\"RODOU O MOUNTED\");\n    const resposta = async () => {\n      await api.get(\"/\").then(res => {\n        console.log(\"resposta: \", res);\n      }).catch(err => {\n        console.log(err);\n      });\n    };\n    console.log(resposta);\n  },\n  components: {\n    TaskUnit,\n    AddTask,\n    ModalTaskInfo\n  },\n  computed: {\n    ...mapState({\n      taskList: state => state.taskList\n    })\n  },\n  data: () => ({\n    editTaskModalVisible: false,\n    editIndex: 0\n  }),\n  methods: {\n    ...mapMutations(['changeTaskStatus', 'deleteTask', 'editTask']),\n    editTaskCall: function (index) {\n      this.editTaskModalVisible = false;\n      this.editTask(index);\n      this.editIndex = 0;\n    },\n    defineEditTask: function (index) {\n      console.log('entrei no edit task');\n      this.$store.commit('newTaskMount', this.$store.state.taskList[index]);\n      this.editIndex = index;\n      this.editTaskModalVisible = true;\n    },\n    changeeditTaskModalVisible: function () {\n      this.editTaskModalVisible = !this.editTaskModalVisible;\n    }\n  }\n};","map":{"version":3,"mappings":"AAqCA;AACA;AACA;AACA;AACA;AAEA;EACA;IACAA;IACA;MACA;QACAA;MACA;QACAA;MACA;IACA;IACAA;EACA;EAEAC;IACAC;IACAC;IACAC;EACA;EAEAC;IACA;MACAC;IACA;EACA;EAGAC;IACAC;IACAC;EACA;EAEAC;IACA,iBACA,oBACA,cACA,WACA;IACAC;MACA;MACA;MACA;IACA;IACAC;MACAZ;MACA;MACA;MACA;IACA;IACAa;MAAA;IAAA;EACA;AACA","names":["console","components","TaskUnit","AddTask","ModalTaskInfo","computed","taskList","data","editTaskModalVisible","editIndex","methods","editTaskCall","defineEditTask","changeeditTaskModalVisible"],"sourceRoot":"src/components/FilteredTasks","sources":["taskList.vue"],"sourcesContent":["<template>\n  <div>\n    <hr>    \n    <AddTask/>    \n    \n    <v-row>\n      <v-col       \n        cols=\"12\"\n        sm=\"6\"\n        md=\"3\"      \n        v-for=\"(task, index) in taskList\" \n        :key=\"index\">\n        <TaskUnit \n              :name = \"taskList[index].name\"\n              :date = \"taskList[index].date\"\n              :description = \"taskList[index].description\"\n              :done = \"taskList[index].done\"\n              @changeStatus = \"changeTaskStatus(index)\"\n              @deleteTask = \"deleteTask(index)\"\n              @editTask = \"defineEditTask(index)\"\n              />\n        <v-dialog\n          v-model=\"editTaskModalVisible\"      \n        >\n          <v-card class=\"pa-10\" outlined shaped>\n            <ModalTaskInfo              \n              label='Editar tarefa:'\n              @save=\"editTaskCall(editIndex)\"\n              @changeAddTask=\"changeeditTaskModalVisible()\"/>\n          </v-card>\n        </v-dialog>  \n      </v-col>\n    </v-row>     \n  </div>\n</template>\n\n<script>\nimport TaskUnit from \"./Task/taskUnit.vue\";\nimport AddTask from './AddTask/addTask.vue';\nimport {mapState, mapMutations} from 'vuex'\nimport ModalTaskInfo from '../common/modal/modalTaskInfo.vue'\nimport api from \"./services/api\"\n\nexport default { \n  async beforeMount(){\n    console.log(\"RODOU O MOUNTED\")\n    const resposta = async () => {\n      await api.get(\"/\").then((res) =>{\n      console.log(\"resposta: \", res)\n      }) .catch((err)  => {\n        console.log(err)\n      }) \n    }  \n    console.log(resposta)\n  },\n\n  components: {\n    TaskUnit,\n    AddTask,\n    ModalTaskInfo\n  },\n\n  computed: {\n    ...mapState({\n      taskList: state => state.taskList\n    })    \n  },\n\n\n  data: () => ({\n    editTaskModalVisible: false,\n    editIndex: 0\n  }),\n\n  methods:{\n    ...mapMutations([\n      'changeTaskStatus',\n      'deleteTask',\n      'editTask'\n    ]),\n    editTaskCall: function(index) {      \n      this.editTaskModalVisible = false;\n      this.editTask(index);\n      this.editIndex = 0;\n    },\n    defineEditTask: function(index){\n      console.log('entrei no edit task')\n      this.$store.commit('newTaskMount',this.$store.state.taskList[index])\n      this.editIndex = index\n      this.editTaskModalVisible = true\n    },\n    changeeditTaskModalVisible: function () {this.editTaskModalVisible = !this.editTaskModalVisible },         \n  }\n};\n\n\n</script>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}